<div>
  <div>
    <div>
      <h1>ジャンル編集</h1>
      <%= form_with model: @genre, url: admin_genre_path do |f| %>
      <table>
        <tr>
          <th>ジャンル名</th>
          <td><%= f.text_field :name %></td>
          <td><%= f.submit "変更を保存", url: admin_genre_path, method: :patch %></td>
        </tr>
      </table>
      <% end %>
    </div>
  </div>
</div>
