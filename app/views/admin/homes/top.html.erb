<h1>注文履歴一覧</h1>
<table>
  <tr>
    <th>購入日時</th>
    <th>購入者</th>
    <th>注文個数</th>
    <th>注文</th>
  </tr>
  <% @orders.each do |orders| %>
  <tr>
    <td>
      <%= link_to admin_order_path(orders.id) do %>
        <%= orders.created_at %>
      <% end %>
    </td>
    <td><%= orders.customer_id %></td>
    <td><!--注文個数!--></td>
    <td><%= orders.status %></td>
  </tr>
  <% end %>
<!--ページネーション!-->